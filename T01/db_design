mySQL:
  Connect: mysql -u root -p
  Create database: CREATE DATABASE database;
  Drop database: DROP DATABASE database;
  Create database backup: mysqldump -u root -p DATABASE_NAME > backup.sql
  Restore a database backup: mysql -u root -p DATABASE_NAME < backup.sql
  Tables: SHOW TABLES;
  Select table: USE table;

